{
  "cli": {
    "version": ">= 0.43.0"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "releaseChannel": "development",
      "env": {
        "ENTRY_FILE": "./__generated__/AppEntry.js"
      }
    },
    "staging": {
      "distribution": "internal",
      "releaseChannel": "staging",
      "env": {
        "ENTRY_FILE": "./__generated__/AppEntry.js"
      }
    },
    "production": {
      "releaseChannel": "production",
      "env": {
        "ENTRY_FILE": "./__generated__/AppEntry.js"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
