    "core-design-system": "0.0.1",
    "dotenv": "~10.0.0",
    "expo": "~44.0.0",
    "expo-auth-session": "~3.5.0",
    "expo-constants": "~13.0.0",
    "expo-status-bar": "~1.2.0",
    "expo-web-browser": "~10.0.3",
    "jest": "^27.4.5",
    "jest-expo": "~43.0.1",
    "native-base": "^3.2.2",
    "react-native": "0.64.3",
    "react-native-safe-area-context": "3.3.2",
