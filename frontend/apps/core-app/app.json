{
  "expo": {
    "name": "NRC Core",
    "slug": "core",
    "owner": "nrc-no",
    "version": "0.0.1",
    "scheme": "no.nrc.core",
    "orientation": "portrait",
    "icon": "./assets/png/nrc_logo_with_text.png",
    "splash": {
      "image": "./assets/png/nrc_logo_with_text.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "no.nrc.core",
      "supportsTablet": true
    },
    "android": {
      "package": "no.nrc.core",
      "adaptiveIcon": {
        "foregroundImage": "./assets/png/nrc_logo_with_text.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/png/nrc_logo_with_text.png"
    },
    "androidStatusBar": {
      "hidden": true,
      "barStyle": "dark-content"
    },
    "androidNavigationBar": {
      "barStyle": "dark-content"
    }
  }
}
