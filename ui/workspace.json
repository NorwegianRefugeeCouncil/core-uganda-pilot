{
  "version": 2,
  "cli": {
    "defaultCollection": "@nrwl/react"
  },
  "defaultProject": "core-web-app",
  "generators": {
    "@nrwl/react": {
      "application": {
        "style": "css",
        "linter": "eslint",
        "strict": true,
        "babel": true
      },
      "component": {
        "style": "css"
      },
      "library": {
        "style": "css",
        "linter": "eslint",
        "strict": true
      }
    }
  },
  "projects": {
    "shared-ui-toolkit": {
      "root": "libs/shared/ui-toolkit",
      "sourceRoot": "libs/shared/ui-toolkit/src",
      "projectType": "library",
      "targets": {
        "build": {
          "executor": "@nrwl/web:package",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/libs/shared/ui-toolkit",
            "tsConfig": "libs/shared/ui-toolkit/tsconfig.lib.json",
            "project": "libs/shared/ui-toolkit/package.json",
            "entryFile": "libs/shared/ui-toolkit/src/index.ts",
            "external": ["react", "react-dom"],
            "rollupConfig": "@nrwl/react/plugins/bundle-rollup",
            "assets": [
              {
                "glob": "libs/shared/ui-toolkit/README.md",
                "input": ".",
                "output": "."
              }
            ]
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/shared/ui-toolkit/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/shared/ui-toolkit"],
          "options": {
            "jestConfig": "libs/shared/ui-toolkit/jest.config.js",
            "passWithNoTests": true
          }
        },
        "storybook": {
          "executor": "@nrwl/storybook:storybook",
          "options": {
            "uiFramework": "@storybook/react",
            "port": 4400,
            "staticDir": ["dist/libs/shared/bootstrap/"],
            "config": {
              "configFolder": "libs/shared/ui-toolkit/.storybook"
            }
          },
          "configurations": {
            "ci": {
              "quiet": true
            }
          }
        },
        "build-storybook": {
          "executor": "@nrwl/storybook:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "uiFramework": "@storybook/react",
            "staticDir": ["dist/libs/shared/bootstrap/"],
            "outputPath": "apps/showcase/src/assets/shared-ui-toolkit",
            "config": {
              "configFolder": "libs/shared/ui-toolkit/.storybook"
            }
          },
          "configurations": {
            "ci": {
              "quiet": true
            }
          }
        }
      }
    },
    "formbuilder": {
      "root": "libs/formbuilder",
      "sourceRoot": "libs/formbuilder/src",
      "projectType": "library",
      "targets": {
        "build": {
          "executor": "@nrwl/web:package",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/libs/formbuilder",
            "tsConfig": "libs/formbuilder/tsconfig.lib.json",
            "project": "libs/formbuilder/package.json",
            "entryFile": "libs/formbuilder/src/index.ts",
            "external": ["react", "react-dom"],
            "rollupConfig": "@nrwl/react/plugins/bundle-rollup",
            "assets": [
              {
                "glob": "libs/formbuilder/README.md",
                "input": ".",
                "output": "."
              }
            ]
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/formbuilder/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/formbuilder"],
          "options": {
            "jestConfig": "libs/formbuilder/jest.config.js",
            "passWithNoTests": true
          }
        },
        "storybook": {
          "executor": "@nrwl/storybook:storybook",
          "options": {
            "uiFramework": "@storybook/react",
            "port": 4400,
            "staticDir": ["dist/libs/shared/bootstrap/"],
            "config": {
              "configFolder": "libs/formbuilder/.storybook"
            }
          },
          "configurations": {
            "ci": {
              "quiet": true
            }
          }
        },
        "build-storybook": {
          "executor": "@nrwl/storybook:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "uiFramework": "@storybook/react",
            "outputPath": "apps/showcase/src/assets/formbuilder",
            "staticDir": ["dist/libs/shared/bootstrap/"],
            "config": {
              "configFolder": "libs/formbuilder/.storybook"
            }
          },
          "configurations": {
            "ci": {
              "quiet": true
            }
          }
        }
      }
    },
    "formrenderer": {
      "root": "libs/formrenderer",
      "sourceRoot": "libs/formrenderer/src",
      "projectType": "library",
      "targets": {
        "build": {
          "executor": "@nrwl/web:package",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/libs/formrenderer",
            "tsConfig": "libs/formrenderer/tsconfig.lib.json",
            "project": "libs/formrenderer/package.json",
            "entryFile": "libs/formrenderer/src/index.ts",
            "external": ["react", "react-dom"],
            "rollupConfig": "@nrwl/react/plugins/bundle-rollup",
            "assets": [
              {
                "glob": "libs/formrenderer/README.md",
                "input": ".",
                "output": "."
              }
            ]
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/formrenderer/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/formrenderer"],
          "options": {
            "jestConfig": "libs/formrenderer/jest.config.js",
            "passWithNoTests": true
          }
        },
        "storybook": {
          "executor": "@nrwl/storybook:storybook",
          "options": {
            "uiFramework": "@storybook/react",
            "port": 4400,
            "staticDir": ["dist/libs/shared/bootstrap/"],
            "config": {
              "configFolder": "libs/formrenderer/.storybook"
            }
          },
          "configurations": {
            "ci": {
              "quiet": true
            }
          }
        },
        "build-storybook": {
          "executor": "@nrwl/storybook:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "uiFramework": "@storybook/react",
            "staticDir": ["dist/libs/shared/bootstrap/"],
            "outputPath": "apps/showcase/src/assets/formrenderer",
            "config": {
              "configFolder": "libs/formrenderer/.storybook"
            }
          },
          "configurations": {
            "ci": {
              "quiet": true
            }
          }
        }
      }
    },
    "showcase": {
      "root": "apps/showcase",
      "sourceRoot": "apps/showcase/src",
      "projectType": "application",
      "targets": {
        "copy-bs": {
          "executor": "./tools/executors/copy-bootstrap-css:copy-bootstrap-css",
          "options": {
            "targetPath": "apps/showcase/src/styles.css"
          }
        },
        "prepare-build": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "commands": ["nx run showcase:copy-bs", "nx run showcase:build"],
            "parallel": false
          }
        },
        "build": {
          "executor": "@nrwl/web:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "baseHref": "/core/",
            "outputPath": "dist/apps/showcase",
            "index": "apps/showcase/src/index.html",
            "main": "apps/showcase/src/main.tsx",
            "polyfills": "apps/showcase/src/polyfills.ts",
            "tsConfig": "apps/showcase/tsconfig.app.json",
            "assets": [
              "apps/showcase/src/favicon.ico",
              "apps/showcase/src/assets"
            ],
            "styles": ["apps/showcase/src/styles.css"],
            "scripts": [],
            "webpackConfig": "@nrwl/react/plugins/webpack"
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/showcase/src/environments/environment.ts",
                  "with": "apps/showcase/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/web:dev-server",
          "options": {
            "buildTarget": "showcase:build"
          },
          "configurations": {
            "production": {
              "buildTarget": "showcase:build:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/showcase/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/showcase"],
          "options": {
            "jestConfig": "apps/showcase/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "showcase-e2e": {
      "root": "apps/showcase-e2e",
      "sourceRoot": "apps/showcase-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/showcase-e2e/cypress.json",
            "tsConfig": "apps/showcase-e2e/tsconfig.e2e.json",
            "devServerTarget": "showcase:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "showcase:serve:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/showcase-e2e/**/*.{js,ts}"]
          }
        }
      }
    },
    "core-ui": {
      "root": "libs/core-ui",
      "sourceRoot": "libs/core-ui/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/core-ui/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/core-ui"],
          "options": {
            "jestConfig": "libs/core-ui/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "shared-api-client": {
      "root": "libs/shared/api-client",
      "sourceRoot": "libs/shared/api-client/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/shared/api-client/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/shared/api-client"],
          "options": {
            "jestConfig": "libs/shared/api-client/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "shared-bootstrap": {
      "root": "libs/shared/bootstrap",
      "sourceRoot": "libs/shared/bootstrap/src",
      "projectType": "library",
      "targets": {
        "build": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "commands": [
              "npx sass -q libs/shared/bootstrap/src/bootstrap.scss libs/shared/bootstrap/build/bootstrap.css",
              "npx sass -q --style=compressed libs/shared/bootstrap/src/bootstrap.scss libs/shared/bootstrap/build/bootstrap.min.css",
              "nx run shared-bootstrap:package"
            ],
            "parallel": false
          }
        },
        "package": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "commands": [
              "rm -rf dist/libs/shared/bootstrap",
              "mkdir -p dist/libs/shared/bootstrap",
              "cp -r libs/shared/bootstrap/build/. dist/libs/shared/bootstrap/"
            ],
            "parallel": false
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/shared/bootstrap/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/shared/bootstrap"],
          "options": {
            "jestConfig": "libs/shared/bootstrap/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "core-web-app": {
      "root": "apps/core-web-app",
      "sourceRoot": "apps/core-web-app/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/web:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/core-web-app",
            "index": "apps/core-web-app/src/index.html",
            "main": "apps/core-web-app/src/main.tsx",
            "polyfills": "apps/core-web-app/src/polyfills.ts",
            "tsConfig": "apps/core-web-app/tsconfig.app.json",
            "assets": [
              "apps/core-web-app/src/favicon.ico",
              "apps/core-web-app/src/assets"
            ],
            "styles": ["apps/core-web-app/src/styles.css"],
            "scripts": [],
            "webpackConfig": "@nrwl/react/plugins/webpack"
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/core-web-app/src/environments/environment.ts",
                  "with": "apps/core-web-app/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/web:dev-server",
          "options": {
            "buildTarget": "core-web-app:build",
            "hmr": true
          },
          "configurations": {
            "production": {
              "buildTarget": "core-web-app:build:production",
              "hmr": false
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/core-web-app/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/core-web-app"],
          "options": {
            "jestConfig": "apps/core-web-app/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "core-web-app-e2e": {
      "root": "apps/core-web-app-e2e",
      "sourceRoot": "apps/core-web-app-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/core-web-app-e2e/cypress.json",
            "tsConfig": "apps/core-web-app-e2e/tsconfig.e2e.json",
            "devServerTarget": "core-web-app:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "core-web-app:serve:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/core-web-app-e2e/**/*.{js,ts}"]
          }
        }
      }
    }
  }
}
