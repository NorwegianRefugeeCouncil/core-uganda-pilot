{
  "expo": {
    "name": "frontend",
    "slug": "frontend",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/png/nrc.png",
    "splash": {
      "image": "./assets/png/nrc.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "dev.core.nrc",
      "supportsTablet": true
    },
    "android": {
      "package": "dev.core.nrc",
      "adaptiveIcon": {
        "foregroundImage": "./assets/png/nrc.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/png/nrc.png"
    },
    "androidStatusBar": {
      "hidden": true,
      "backgroundColor": "#728346",
      "barStyle": "dark-content"
    },
    "androidNavigationBar": {
      "visible": "immersive",
      "barStyle": "dark-content"
    }
  }
}
